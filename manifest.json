{
  "name": "affinity-tracker",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "A chrome extension that track user affinity for specified product keywords",
  "content_scripts": [
    {
      "matches": [
        "http://*.urbanoutfitters.com/*",
        "https://*.urbanoutfitters.com/*"
      ],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": ["affinity_tracker.js"]
}
